<odoo>
<record id="gim_clase_action" model="ir.actions.act_window">
    <field name="name">Clase</field>
    <field name="res_model">gim.clase</field>
    <field name="view_mode">tree,form</field>

</record>

<record id="gim_clase_tree_view" model="ir.ui.view">
    <field name="name">gim.clase.tree</field>
    <field name="model">gim.clase</field>
    <field name="arch" type="xml">
        <tree string="Clases">
                <field name="nombre"/>
                <field name="fecha"/>
        </tree>
    </field>

</record>

<record id="gim_clase_form_view" model="ir.ui.view">
    <field name="name">gim.clase.tree</field>
    <field name="model">gim.clase</field>
    <field name="arch" type="xml">
        <form string="Clases">
            <sheet>
                <field name="nombre"/>
                <field name="fecha"/>
                <!--<field name="autor_id"/>-->
                <!--<field name="prestamo_id"/>-->
                <!--<field name="socio_fav_ids" domain="[('es_socio', '=', True)]"/>-->
            </sheet>
            
        </form>
    </field>

</record>


</odoo>